#!/usr/bin/env python
print "1,2,3".split(",")

print [map(int, s) for s in "1,2,3".split(",")]

print map(int, "1,2,3".split(","))

path="SCHEMA/public/TABLE/tablename/INDEX/indexname"
print "/".join(path.split("/")[:-1]+["indexname2"])

print "/".join([path.split("/")[1]]+["public2"]+path.split("/")[2:])