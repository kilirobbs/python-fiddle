#!/usr/bin/env python
# -*- coding: utf-8 -*-
from jinja2 import Environment

source="""
{% for i in list -%}
    {{ i }}
{%- endfor %}
"""
tmpl = Environment().from_string(source)
print tmpl.render(list=[10,11,12])

source="""
{%- for i in list -%}
    {{ i }}
{% endfor %}
"""
tmpl = Environment().from_string(source)
print tmpl.render(list=[10,11,12])